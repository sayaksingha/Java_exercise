//recursion approch

class as{
    static int power(int a,int b){
        if(b==0){
            return 1;
        }
       int c= power(a, b/2);
        if(b%2==0){
            System.out.println(c*c);
            return c*c;
        }
        else{
            System.out.println(c*c*a);
            return c*c*a;
        }
       
    }
}
class kdnfk{
    public static void main(String[] args) {
      int vaal= as.power(4, 5);
      System.out.println(vaal);
    }
}
